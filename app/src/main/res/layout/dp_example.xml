<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:n1="http://schemas.android.com/apk/res/android"
    n1:layout_width="fill_parent"
    n1:layout_height="fill_parent"
    n1:background="@android:color/white"
    n1:gravity="center">

    <LinearLayout
        n1:layout_width="wrap_content"
        n1:layout_height="wrap_content"
        n1:orientation="vertical">

        <LinearLayout
            n1:id="@+id/give_us_a_review_landmine_main_layout"
            n1:layout_width="wrap_content"
            n1:layout_height="wrap_content"
            n1:gravity="center"
            n1:orientation="vertical"
            n1:paddingLeft="43.0dip"
            n1:paddingTop="50.0dip"
            n1:paddingRight="43.0dip"
            n1:paddingBottom="27.0dip">

            <TextView
                n1:layout_width="wrap_content"
                n1:layout_height="wrap_content"
                n1:text="Intuit"
                n1:textColor="@android:color/black"
                n1:textSize="40.0dip" />

            <LinearLayout
                n1:layout_width="wrap_content"
                n1:layout_height="wrap_content"
                n1:layout_marginTop="-10.0dip"
                n1:orientation="horizontal"
                n1:paddingBottom="15.0dip">

                <TextView
                    n1:layout_width="wrap_content"
                    n1:layout_height="wrap_content"
                    n1:includeFontPadding="false"
                    n1:text="♡"
                    n1:textColor="#ffed6c27"
                    n1:textSize="70.0dip"
                    n1:textStyle="bold" />

                <TextView
                    n1:layout_width="wrap_content"
                    n1:layout_height="wrap_content"
                    n1:includeFontPadding="false"
                    n1:text="U"
                    n1:textColor="@android:color/black"
                    n1:textSize="70.0dip" />
            </LinearLayout>

            <TextView
                n1:id="@+id/give_us_a_review_landmine_text_1"
                n1:layout_width="wrap_content"
                n1:layout_height="wrap_content"
                n1:gravity="center"
                n1:paddingBottom="12.0dip"
                n1:text="Rate us so we can grow and help more people get their finances in check"
                n1:textColor="@android:color/black"
                n1:textSize="16.0dip" />

            <TextView
                n1:id="@+id/give_us_a_review_landmine_text_2"
                n1:layout_width="fill_parent"
                n1:layout_height="wrap_content"
                n1:gravity="center"
                n1:text="★★★★★"
                n1:textColor="#ff747474"
                n1:textSize="22.0dip"
                n1:textStyle="bold" />

            <Button
                n1:id="@+id/give_us_a_review_landmine_button"
                n1:layout_width="fill_parent"
                n1:layout_height="wrap_content"
                n1:layout_gravity="center"
                n1:layout_marginTop="25.0dip"
                n1:background="#ff0ac775"
                n1:gravity="center"
                n1:includeFontPadding="false"
                n1:minWidth="120.0dip"
                n1:padding="8.0dip"
                n1:singleLine="true"
                n1:text="Rate"
                n1:textColor="@android:color/white"
                n1:textSize="15.0dip"
                n1:visibility="visible" />
        </LinearLayout>
    </LinearLayout>
</RelativeLayout>
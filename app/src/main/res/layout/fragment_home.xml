<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:n1="http://schemas.android.com/apk/res/android"
    n1:id="@+id/coordinatorlayout"
    n1:layout_width="fill_parent"
    n1:layout_height="fill_parent">

    <ScrollView
        style="@style/Scrollview_Style"
        n1:background="#2220b2aa">

        <RelativeLayout
            n1:layout_width="fill_parent"
            n1:layout_height="fill_parent"
            n1:gravity="center_horizontal"
            n1:orientation="vertical">

            <GridView
                n1:id="@+id/gridview_home"
                n1:layout_width="fill_parent"
                n1:layout_height="fill_parent"
                n1:animateLayoutChanges="true"
                n1:columnWidth="100.0dip"
                n1:gravity="center"
                n1:numColumns="auto_fit"
                n1:scrollbarThumbVertical="@null"
                n1:stretchMode="columnWidth" />
        </RelativeLayout>
    </ScrollView>
</androidx.coordinatorlayout.widget.CoordinatorLayout>